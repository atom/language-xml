'.text.xml':
  'CDATA':
    'prefix': 'cdata'
    'body': '<![CDATA[$0]]>'
  'XML Processing Instruction':
    'prefix': 'xml'
    'body': '<?xml version="1.0" encoding="UTF-8"?>'
  'Long Attribute Tag':
    'prefix': '<a'
    'body': '<${1:name} ${2:attr="value"}>$4\n</${3:name}>'
  'Long Tag':
    'prefix': '<'
    'body': '<${1:name}>$3</${2:name}>'
  'Short Tag':
    'prefix': '>'
    'body': '<${1:name} />'
